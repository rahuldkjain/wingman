{
  "manifest_version": 3,
  "name": "Wingman",
  "version": "1.0.0",
  "description": "Your flight search assistant.",
  "action": {
    "default_popup": "index.html",
    "default_title": "Wingman"
  },
  "permissions": [
    "storage",
    "tabs"
  ],
  "icons": {
    "16": "vite.svg",
    "48": "vite.svg",
    "128": "vite.svg"
  }
}

